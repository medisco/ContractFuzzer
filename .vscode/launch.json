{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/contractfuzzer/cmd/contractfuzzer",
            "env": {
                "CONTRACT_DIR": "${workspaceFolder}/examples/delegatecall_dangerous",
                "TESTER_HOST": "localhost",
                "PORT": "8888"
            },
            "args": [
                "-abi_dir=${workspaceFolder}/examples/delegatecall_dangerous/verified_contract_abis",
                "-out_dir=${workspaceFolder}/examples/delegatecall_dangerous/verified_contract_abis_fuzz",
                "-contract_list=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/config/contracts.list",
                "-addr_seeds=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/config/addr_seeds.json",
                "-int_seeds=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/config/int_seeds.json",
                "-uint_seeds=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/config/uintSeed.json",
                "-string_seeds=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/config/string_seeds.json",
                "-byte_seeds=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/config/byte_seeds.json",
                "-bytes_seeds=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/config/bytes_seeds.json",
                "-addr_map=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/config/addrmap.csv",
                "-abi_sigs_dir=${workspaceFolder}/examples/delegatecall_dangerous/verified_contract_abi_sigs",
                "-bin_sigs_dir=${workspaceFolder}/examples/delegatecall_dangerous/verified_contract_bin_sigs",
                "-tester_port=http://localhost:8088/",
                "-listen_port=:8888",
                "-reporter=${workspaceFolder}/examples/delegatecall_dangerous/fuzzer/reporter"
            ]
        }
    ]
}